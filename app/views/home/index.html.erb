<div id='wrapper'>
<%if @poll%>
<ul>
  <%[[300,250], [160,600], [120,300]].each do |width, height|%>
    <li <%="class=active" if width == 160%>>
      <a href='#' class='resize-link' data-width=<%=width%> data-height=<%=height%>><%=width%> x <%=height%></a>
    </li>
  <%end%>
</ul>
<div class='iframe-wrapper'>
  <iframe src='/en/partner_polling/polls/<%=@poll.id%>' width='100%' height='100%' frameborder=0/>
</div>
<%end%>
</div>